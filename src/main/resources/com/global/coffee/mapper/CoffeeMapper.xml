<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.global.coffee.mapper.CoffeeMapper">

	<insert id="insertCoffee">

		INSERT INTO COFFEE (
		UUID
		,NAME
		,AMOUNT
		,ETC

		) VALUES (
		#{uuid}
		,#{name}
		,#{amount}
		,#{etcStr}

		)


	</insert>

</mapper>
